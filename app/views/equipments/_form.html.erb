<ul>
<%= form_for :equipment, url: equipments_path do |f| %>

	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>

	<div class="form-group">
	<%= f.label :age_range %>
	<%= f.text_field :age_range %>
	</div>

	<div class="form-group">
		<%= f.label :description %>
		<%= f.text_area :description, size: "60x12" %>
	</div>

	<div class="form-group">
		<%= f.submit %>
	</div>

<% end %>
</ul>
