<ul id="search">
<%= form_tag(parks_path, method: "get") do %>
  <%= button_tag "Find me!", value: "search" %>
  <%= label_tag(:location, "Search Near:") %>
	<%= text_field_tag(:location, params[:location], placeholder: "City, State")  %>
  <%= label_tag(:distance, "Distance:") %>
	<%= text_field_tag(:distance, params[:distance], placeholder: "5km")  %>
  <%= submit_tag("Search") %>
<% end %>

</ul>